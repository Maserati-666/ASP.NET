@model IEnumerable<SpisokStudents.Models.Bigtable>

@{
    ViewData["Title"] = "AllStudent";
}

<h3>Сводная таблица</h3>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NameStudent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NameCourse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RatingStud)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NameStudent)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NameCourse)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RatingStud)
                </td>
        </tr>
}
    </tbody>
</table>
